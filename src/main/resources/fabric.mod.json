{
  "schemaVersion": 1,
  "id": "toastermod",
  "version": "${version}",

  "custom": {
    "cardinal-components": [
      "toastermod:proto_comp",
      "toastermod:proto_tick_comp"
    ]
  },

  "name": "Rise of the Toasters",
  "description": "Thanks to Torch for making some of the textures used in the mod and Toaster for making the main player model and textures for it.",
  "authors": [
    "Ned Hunter",
    "Torch the Dragon",
    "Toaster"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/toastermod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.oyosite.ticon.toastermod.ToasterMod"
    ],
    "client": [
      "com.oyosite.ticon.toastermod.ToasterModClient"
    ],
    "cardinal-components": [
      "com.oyosite.ticon.toastermod.component.EntityEntrypoint"
    ]
  },
  "mixins": [
    "toastermod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16",
    "geckolib3": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
